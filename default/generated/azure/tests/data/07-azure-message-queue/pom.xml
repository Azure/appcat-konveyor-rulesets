		    <groupId>software.amazon.awssdk</groupId>
		    <artifactId>sns</artifactId>
		    <version>2.40.8</version>
		    <scope>test</scope>
		</dependency>
		<!-- https://mvnrepository.com/artifact/com.amazonaws/aws-java-sdk-sns -->
